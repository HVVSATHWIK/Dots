{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ai\\/caption\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ai\\/chat\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ai\\/design-variations\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ai\\/generate\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ai\\/image\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ai\\/listing-pack\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/trust\\/mint\\/?$",
			"dest": "_render"
		},
		{
			"src": "^(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		}
	]
}